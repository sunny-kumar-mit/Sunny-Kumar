#include <stdio.h>

int main(){
    int a;
    printf("Enter any year:\n");
    scanf("%d",&a);

    if (a%4==0 && a%100!=0 || a%400==0)
    {
        printf("The year you entered %d is leap year\n",a);
    }
    else 
    {
        printf("The year you entered %d is not a leap year\n",a);
    }
    return 0;
}
